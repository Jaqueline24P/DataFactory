{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "palominotfactory"
		},
		"AzureDatabricks_DataFactory2_accessToken": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accessToken\"de \"AzureDatabricks_DataFactory2\""
		},
		"AzureDatabricks_Datafactory_accessToken": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accessToken\"de \"AzureDatabricks_Datafactory\""
		},
		"AzureDatabricks_Datafactory3_accessToken": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accessToken\"de \"AzureDatabricks_Datafactory3\""
		},
		"InputFieldsCopy_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accountKey\"de \"InputFieldsCopy_ADLS\""
		},
		"InputFiles_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accountKey\"de \"InputFiles_ADLS\""
		},
		"OutputFiles_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accountKey\"de \"OutputFiles_ADLS\""
		},
		"AzureDatabricks_DataFactory2_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0203-174853-m4e8va9n"
		},
		"AzureDatabricks_Datafactory_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0203-174853-m4e8va9n"
		},
		"AzureDatabricks_Datafactory3_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0203-174853-m4e8va9n"
		},
		"InputFieldsCopy_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://palominotstorage.dfs.core.windows.net/"
		},
		"InputFiles_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://palominotstorage.dfs.core.windows.net/"
		},
		"OutputFiles_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://palominotstorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "palominotfactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks_DataFactory2_accessToken": {
						"value": "[parameters('AzureDatabricks_DataFactory2_accessToken')]"
					},
					"AzureDatabricks_Datafactory_accessToken": {
						"value": "[parameters('AzureDatabricks_Datafactory_accessToken')]"
					},
					"AzureDatabricks_Datafactory3_accessToken": {
						"value": "[parameters('AzureDatabricks_Datafactory3_accessToken')]"
					},
					"InputFieldsCopy_ADLS_accountKey": {
						"value": "[parameters('InputFieldsCopy_ADLS_accountKey')]"
					},
					"InputFiles_ADLS_accountKey": {
						"value": "[parameters('InputFiles_ADLS_accountKey')]"
					},
					"OutputFiles_ADLS_accountKey": {
						"value": "[parameters('OutputFiles_ADLS_accountKey')]"
					},
					"AzureDatabricks_DataFactory2_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_DataFactory2_properties_typeProperties_existingClusterId')]"
					},
					"AzureDatabricks_Datafactory_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_Datafactory_properties_typeProperties_existingClusterId')]"
					},
					"AzureDatabricks_Datafactory3_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_Datafactory3_properties_typeProperties_existingClusterId')]"
					},
					"InputFieldsCopy_ADLS_properties_typeProperties_url": {
						"value": "[parameters('InputFieldsCopy_ADLS_properties_typeProperties_url')]"
					},
					"InputFiles_ADLS_properties_typeProperties_url": {
						"value": "[parameters('InputFiles_ADLS_properties_typeProperties_url')]"
					},
					"OutputFiles_ADLS_properties_typeProperties_url": {
						"value": "[parameters('OutputFiles_ADLS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}